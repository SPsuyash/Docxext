{
    "manifest_version": 3,
    "name": "Docxext",
    "version": "1.0",
    "description": "View DOCX files within Firefox.",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "48": "icon48.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": ["viewer.html"]
  }
  