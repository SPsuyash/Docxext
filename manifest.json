{
    "manifest_version": 3,
    "name": "Docxext",
    "version": "1.0",
    "description": "View DOCX files within Firefox.",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "archlogo.jpg",
        "48": "archlogo.jpg",
        "128": "archlogo.jpg"
      }
    },
    "icons": {
      "48": "archlogo.jpg"
    },
    "web_accessible_resources": ["viewer.html", "jszip.js"]
  }
  